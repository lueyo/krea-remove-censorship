{
  "manifest_version": 2,
  "name": "Kreauncen Extension",
  "version": "0.1",
  "description": "Removes nsfw block in krea realtime.",
  "author": "lueyo",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.krea.ai/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://www.krea.ai/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "kreauncen-extension@lueyo.es",
      "strict_min_version": "57.0"
    }
  }
}